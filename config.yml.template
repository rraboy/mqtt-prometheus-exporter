mqtt:
    host: localhost
    port: 1883
    clientID: mqtt-prometheus-exporter
    topics:
        uptime: mqtt-prometheus-exporter/state/uptime
prom:
    http:
        addr: :9123
    gauges:
        - metricName: mqtt_values_temperature
          mqttName: +/values/temperature
          help: temperature
          labels:
            sensor: $0
            static-label: temperature

